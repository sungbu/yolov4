{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debug with Python 3.11",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/train.py",
            "pythonPath": "/Library/Frameworks/Python.framework/Versions/3.11/Resources/Python.app/Contents/MacOS/Python",
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false,
            "debugOptions": [],
            "logToFile": true,
            "console": "internalConsole",
            "exceptionHandling": {
                "ignore": [],
                "alwaysBreak": {}
            },
            "showReturnValue": true
        }
    ]
}
